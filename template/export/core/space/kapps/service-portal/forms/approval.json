        {
          "name": "Load Summary Review",
          "code": "// Loads Data from the 'Summary Review Data' Section into the 'Summary Review HTML' for display to the user.\n// Use descriptive variable names and proper declarations\nconst displaySection = K('section[Summary Review Data]').element();\nconst reviewTarget = K('content[Summary Review HTML]').element();\n\n// Early exit if elements don't exist\nif (!displaySection || !reviewTarget) {\n  console.error('Required elements not found');\n  return;\n}\n\n// Get field names from labels\nconst labels = displaySection.querySelectorAll('label.field-label');\nconst fieldNames = Array.from(labels)\n  .map(label => {\n    const forAttr = label.getAttribute('for');\n    const input = forAttr ? document.getElementById(forAttr) : null;\n    return input?.getAttribute('name');\n  })\n  .filter(name => name);\n\n// Create outer container for non-Summary fields\nconst outerContainer = document.createElement('div');\nouterContainer.className = 'bg-base-200 rounded-box px-2 py-1.5 md:py-3';\n\n// Create dl element - changed to single column\nconst dl = document.createElement('dl');\ndl.className = 'max-md:text-xs flex flex-col gap-3';\ndl.style.overflowWrap = 'anywhere';\n\n// Build review content\nfieldNames.forEach(fieldName => {\n  const field = K(`field[${fieldName}]`);\n  const value = field.value();\n  const label = field.label();\n  \n  if (!value) return;\n  \n  // Check if this is the Summary field\n  const isSummary = label === 'Summary';\n  \n  if (isSummary) {\n    // Append Summary directly to reviewTarget as first child\n    const summaryDiv = document.createElement('div');\n    summaryDiv.className = 'text-lg md:text-xl font-semibold';\n    \n    // Add value with line breaks\n    const lines = value.split('\\n');\n    lines.forEach((line, index) => {\n      summaryDiv.appendChild(document.createTextNode(line));\n      if (index < lines.length - 1) {\n        summaryDiv.appendChild(document.createElement('br'));\n      }\n    });\n    \n    reviewTarget.appendChild(summaryDiv);\n  } else {\n    // Create field container div for non-Summary fields\n    const fieldDiv = document.createElement('div');\n    fieldDiv.className = 'flex flex-col gap-0.5 md:gap-1';\n    \n    const dt = document.createElement('dt');\n    dt.className = 'text-base-content/60 font-medium';\n    dt.textContent = label;\n    fieldDiv.appendChild(dt);\n    \n    // Create dd (value) element\n    const dd = document.createElement('dd');\n    \n    // Add value with line breaks\n    const lines = value.split('\\n');\n    lines.forEach((line, index) => {\n      dd.appendChild(document.createTextNode(line));\n      if (index < lines.length - 1) {\n        dd.appendChild(document.createElement('br'));\n      }\n    });\n    \n    fieldDiv.appendChild(dd);\n    dl.appendChild(fieldDiv);\n  }\n});\n\n// Only append outer container if there's data in dl\nif (dl.children.length > 0) {\n  outerContainer.appendChild(dl);\n  reviewTarget.appendChild(outerContainer);\n}",
          "integrationResourceName": null,
          "action": "Custom",
          "integrationResourceProperty": "",
          "type": "Load"
        }
