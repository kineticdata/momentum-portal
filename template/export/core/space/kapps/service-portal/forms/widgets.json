      "events": [
        {
          "name": "Load Search Widget",
          "code": "bundle.widgets.Search?.({ \n  container: K('section[Requested For Widget]').element(), \n  config: {\n    integration: {\n      kappSlug: K('kapp').slug(),\n      formSlug: K('form').slug(),\n      integrationName: 'Search Users',\n      listProperty: 'users',\n      parameters: [{ name: 'Requested For' }]\n    },\n    optionToValue: function(o) { return o.username; },\n    optionToLabel: function(o) { return o.displayName || o.username; },\n    initialSelection: K('field[Requested For]').value() ? {\n      username: K('field[Requested For Name]').value()\n    } : null,\n    onChange: function(obj, value) {\n      K('field[Requested For]').value(value);\n      K('field[Requested For Name]').value(obj ? obj.displayName : '');\n    },\n    placeholder: 'Search users...'\n  }, \n  id: 'requested-for' \n});\n\nbundle.widgets.Search?.({ \n  container: K('section[Static Search Widget]').element(), \n  config: {\n    options: [\n      { value: 'low', label: 'Low Priority'},\n      { value: 'medium', label: 'Medium Priority'},\n      { value: 'high', label: 'High Priority'},\n      { value: 'urgent', label: 'Urgent Priority'},\n    ],\n    search: { fields: [{ name: 'value' }] },\n    placeholder: 'Find priority',\n    minSearchLength: 0\n  }, \n  id: 'static-search' \n});",
          "integrationResourceName": "",
          "action": "Custom",
          "integrationResourceProperty": null,
          "type": "Load"
        },
        {
          "name": "Load Markdown Widget",
          "code": "bundle.widgets.Markdown?.({ \n  container: K('section[Markdown]').element(), \n  field: K('field[Markdown Content]'), \n  config: {}, \n  id: 'markdown' \n});",
          "integrationResourceName": null,
          "action": "Custom",
          "integrationResourceProperty": "",
          "type": "Load"
        },
        {
          "name": "Load Signature Widget",
          "code": "bundle.widgets.Signature?.({\n  container: K('section[Signature]').element(), \n  field: K('field[Signature File]'), \n  config: {\n    modalTitle: 'Provide Your Signature',\n    signaturePadLabel: 'Your Signature Please',\n    agreementText: null,\n    savedButtonLabel: 'Save Signature',\n    savedFileName: 'my_signature',\n    buttonLabel: 'Sign Here'\n  }, \n  id: 'signature'\n});",
          "integrationResourceName": null,
          "action": "Custom",
          "integrationResourceProperty": "",
          "type": "Load"
        },
        {
          "name": "Load Table Widget",
          "code": "const table = bundle.widgets.Table?.({ \n  container: K('section[Table]').element(), \n  field: K('field[Table Data]'),\n  config: {\n    integration: {\n      kappSlug: K('kapp').slug(),\n      formSlug: K('form').slug(),\n      integrationName: 'Addresses',\n      listProperty: 'Data',\n      errorProperty: 'Error'\n    },\n    // data: [], \n    rowTransform: ({ Values, ...row }) => ({ ...row, ...Values }),\n    columns: [\n      {\n        label: 'Id',\n        property: 'Handle',\n        displayTransform: (value, row, index) => value + '(' + row['Core State'] + ')',\n        onClick: editRow,\n        sortable: true,\n        toggleable: false,\n        sortTransform: (value, row, index) => value,\n        filterable: false,\n      },{\n        label: 'Address',\n        property: 'Address Line 1',\n        sortable: true,\n        sortTransform: (value) => (value || '').match(/^\\d*\\s*(.*)$/)[1],\n        filterable: true,\n        filterFn: function(query, value) {\n          const match = (value || '').match(/^\\d*\\s*(.*)$/);\n          if (match[1] && match[1].toLowerCase().startsWith(query.toLowerCase())) return true;\n          return false;\n        }\n      },{\n        label: 'City',\n        property: 'City',\n        sortable: true,\n        sortTransform: (value, row, index) => value,\n        filterable: true,\n        filterOperator: 'matches',\n      },{\n        label: 'State',\n        property: 'State',\n        sortable: true,\n        sortTransform: (value, row, index) => value,\n        filterable: true,\n        filterOperator: 'equals',\n      },{\n        label: 'Zip Code',\n        property: 'Zip Code',\n        sortable: false,\n        filterable: true,\n        filterOperator: 'startsWith',\n      }\n    ],\n    actions: [\n      { label: 'Edit', onClick: editRow, icon: 'edit' },\n      { label: 'Delete', onClick: deleteRow, icon: 'trash' },\n    ],\n    pageSize: 3,\n    // defaultSort: [0, 'asc'],\n    messages: {\n      loading: 'Loading Data',\n      empty: 'No Data',\n    }\n  }, \n  id: 'table' \n});\n\nfunction editRow(value, row, index) {\n  console.log('EDIT', { value, row, index, table });\n}\n\nfunction deleteRow(value, row, index) {\n  console.log('DELETE', { value, row, index, table });\n}",
          "integrationResourceName": null,
          "action": "Custom",
          "integrationResourceProperty": "",
          "type": "Load"
        }
      ],
